{
  "name": "pond-lambda",
  "version": "0.1.0",
  "bin": {
    "pond-lambda": "bin/pond-lambda.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build:lambda && cdk deploy",
    "build:lambda": "cargo lambda build --release --manifest-path ../../pond/pond-planner/Cargo.toml && cargo lambda build --release --manifest-path ../../pond/pond-duckling/Cargo.toml"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "aws-cdk": "2.100.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
